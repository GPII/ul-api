    "gpii-express-user": "git://github.com/the-t-in-rtf/gpii-express-user.git#6580b1167be05ffd935c498c02590a69eaa0561d",
    "gpii-handlebars": "git://github.com/the-t-in-rtf/gpii-handlebars.git#c66207050ba764cbdbce401db725d2932c8fcbce",
    "gpii-json-schema": "git://github.com/the-t-in-rtf/gpii-json-schema.git#b4fb0746633683eb3c8fd6ea0719ee4c1bcd52f5",
