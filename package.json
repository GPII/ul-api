{
  "name": "ul-api",
  "version": "1.0.0",
  "description": "The Unified Listing API and tests.",
  "main": "index.js",
  "scripts": {
    "bundle": "npm run bundle:http; npm run bundle:https; npm run bundle:url",
    "bundle:http": "node node_modules/browserify/bin/cmd.js -s http node_modules/http-browserify/index.js -o browserify/http.js && node node_modules/uglify/bin/uglify -s browserify/http.js -o browserify/http.min.js ",
    "bundle:https": "node node_modules/browserify/bin/cmd.js -s https node_modules/https-browserify/index.js -o browserify/https.js && node node_modules/uglify/bin/uglify -s browserify/https.js -o browserify/https.min.js ",
    "bundle:url": "node node_modules/browserify/bin/cmd.js -s urlModule node_modules/url/url.js -o browserify/urlModule.js && node node_modules/uglify/bin/uglify -s browserify/urlModule.js -o browserify/urlModule.min.js ",
    "postinstall": "npm run bundle",
    "pretest": "npm run bundle",
    "test": "npm run tests:browser; npm run tests:node",
    "tests:node": "node tests/node-tests.js",
    "tests:browser": "testem ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GPII/ul-api.git"
  },
  "author": "Tony Atkins <tony@raisingthefloor.org>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://issues.gpii.net/browse/GPII"
  },
  "homepage": "https://github.com/GPII/ul-api",
  "dependencies": {
    "gpii-binder": "git://github.com/the-t-in-rtf/gpii-binder#f1ade44e0be4990a2400e54061a5557e634a13a8",
